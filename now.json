{
  "name": "conflux-bounty",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    { "src": "dist/**/*", "use": "@now/static" }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "http://bountytest.conflux-chain.org/api"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false,
    "silent": false
  }
}